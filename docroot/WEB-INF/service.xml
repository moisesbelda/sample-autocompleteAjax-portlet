<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.liferay.sample">
	<author>Moises Belda</author>
	<namespace>autoCompleteAjax</namespace>
	<entity name="Poblacion" local-service="true" remote-service="false" uuid="false" trash-enabled="false">
		<!-- Primary key column -->
		<column name="poblacionId" type="long" primary="true" />

		<!-- Foreign key columns -->
		<column name="companyId" type="long" />
		<column name="groupId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other columns -->		
		<column name="description" type="String" />
	</entity>
</service-builder>